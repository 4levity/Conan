<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse.ant.import?>
<project default="create_run_jar" name="Create Runnable Jar for Project Conan">
    <target depends="build" name="jar">
        <jar destfile="bin/conan.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="info.jlibrarian.conan.ConanApp"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="bin/info/jlibrarian"/>
        </jar>
    </target>
</project>
